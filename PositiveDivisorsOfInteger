using System;
using System.Collections.Generic;
using System.Text;

namespace PositiveDivisors
{
    public class Divisor
    {
        public IEnumerable<int> GetPositiveDivisors(int inputNumber)
        {
            for (int i = 1; i * i <= inputNumber; i++)
            {
                if (inputNumber % i == 0)
                {
                    yield return i;
                    if (i * i != inputNumber)
                        yield return inputNumber / i;
                }
            }
        }
    }
}
